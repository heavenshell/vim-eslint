# vim:set et sw=4 ts=4 tw=79:
Execute ('Eslint'):
  :edit ./src/app.ts
  :call eslint#run('a')
  :sleep 3

  let qf = getqflist()
  AssertEqual len(qf), 5

Execute ('Eslint with tsuquyomi'):
  :edit ./src/app.ts
  :let g:tsuquyomi_disable_quickfix = 1
  :sleep 3
  :write
  :sleep 3

  let qf = getqflist()
  AssertEqual len(qf), 9
